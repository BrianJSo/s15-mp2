<div class="activity_container">
    {{!-- <h1>{{title}}</h1> --}}
    <div id="watchedContainer" class="list_container">
        <h2>Watched</h2>
        <div id="watchedList" class="list">
            {{#each watched}}
                {{> tile auctionID}}
            {{/each}}
        </div>
    </div>
    <div id="bidsContainer" class="list_container">
        <h2>Active Bids</h2>
        <div id="bidsList" class="list">
            {{#each bids}}
                {{> tile}}
            {{/each}}
        </div>
    </div>
</div>